{
    "name": "indigophp/queue",
    "description": "Manage work queues with PHP",
    "license": "MIT",
    "keywords": ["queue", "worker", "job"],
    "homepage": "https://indigophp.com",
    "authors": [
        {
            "name": "Márk Sági-Kazár",
            "email": "mark.sagikazar@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "psr/log": "~1.0",
        "jeremeamia/SuperClosure": "1.0.1",
        "symfony/options-resolver": "v2.4.0"
    },
    "require-dev": {
        "pda/pheanstalk": "2.1.0",
        "videlalvaro/php-amqplib": "2.2.*",
        "iron-io/iron_mq": "1.4.8",
        "mockery/mockery": "dev-master@dev"
    },
    "suggest": {
        "pda/pheanstalk": "PHP client for beanstalkd queue",
        "videlalvaro/php-amqplib": "This library is a pure PHP implementation of the AMQP protocol. It's been tested against RabbitMQ.",
        "iron-io/iron_mq": "Client binding for IronMQ (hosted message queue)"
    },
    "autoload": {
        "psr-4": {
            "Indigo\\Queue\\": ["src/", "tests/"]
        }
    }
}
